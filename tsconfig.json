{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "target": "ES6",
        "module": "ES6",
        "jsx": "react",
        "jsxFactory": "vixi.draw",
        "baseUrl": ".",
        "paths": {
            "@state": ["src/state.ts"],
            "@assets/*": ["src/assets/*"],
            "@vixi": ["src/engine/index.ts"]
        }
    }
}
